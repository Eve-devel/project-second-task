//-
  options.item - массив объектов со свойствами:
    icon:'' (иконка Material icons);
    header:'' (заголовок пункта);
    text:'' (пояснение пункта);

mixin icon-text-list(options={})
  .icon-text-list
    each item in options.items
      .icon-text-list__item
        p.icon-text-list__icon.material-icons=item.icon
        .icon-text-list__texts
          span.icon-text-list__header=item.header
          p=item.text
